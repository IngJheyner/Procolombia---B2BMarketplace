    "require-dev": {
        "drupal/core-dev": "^9.0.0@dev",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.7",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "drupal/drupal-library-installer-plugin": true,
            "wikimedia/composer-merge-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "composer.libraries.json"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "TypeError": "https://www.drupal.org/files/issues/2021-10-06/route-builder-undefined-null-path-3196793-11.patch",
                "Create a theme suggestion for taxonomy terms by view mode": "https://www.drupal.org/files/issues/core-theme-suggestion-for-taxonomy-view-modes-2767243-14.patch",
                "Make exposed filter values available as tokens for text areas": "https://www.drupal.org/files/issues/2021-07-02/2761273-39.patch",
                "Drupal Core Argument 1 passed ": "https://www.drupal.org/files/issues/2022-02-28/2701575-14.patch",
                "htmlspecialchars": "https://www.drupal.org/files/issues/2022-03-18/fix-for-php-greater-8-1-3255637.patch",
                "User context missing": "https://www.drupal.org/files/issues/2019-10-23/3056234-10.patch",
		            "recovery password link" : "patches/unicode.patch"
            },
            "drupal/redirect": {
                "Loop redirect fails on save nodes": "patches/loop-redirect-fails-on-save-node.patch"
            },
            "drupal/select2": {
                "change directory select 2 library": "patches/library-select-2-b2b.patch"
            },
            "drupal/taxonomy": {
                "Create a theme suggestion for taxonomy terms by view mode": "https://www.drupal.org/files/issues/core-theme-suggestion-for-taxonomy-view-modes-2767243-14.patch"
            },
            "drupal/views": {
                "Cannot use relationship for rendered entity on Views": "https://www.drupal.org/files/issues/2021-05-13/8.9.x-2457999-267-views-relationship-rendered-entity.patch"
            },
            "drupal/dblog":{
                "Make exposed filter values available as tokens for text areas":"https://www.drupal.org/files/issues/2021-07-02/2761273-39.patch"
            },
            "drupal/redis":{
                "If Redis connection is not configured, the report page shows warnings":"https://www.drupal.org/files/issues/2021-04-07/redis-report_warnings-3207574-2.patch"
            },
            "drupal/google_analytics":{
                "Passing null to parameter #2": "https://www.drupal.org/files/issues/2022-06-03/google_analytics-jsonserialize-code-standard-fixes-3258588-20.patch"
            },
            "drupal/color_field":{
                 "Warning Implicit color_field ColorRGB ": "https://www.drupal.org/files/issues/2022-02-23/3265958-implicit-conversion-float-int.patch"
            },
            "drupal/mailchimp_transactional":{
              "The Mailchimp Transactional library has not been installed correctly": "https://www.drupal.org/files/issues/2022-04-07/mailchimp_transaction.install-ApiClient-misspelled.patch"
            },
            "drupal/webform":{
              "eprecated function: preg_match(): Passing null to parameter #2 ($subject) of type string is deprecated in webform_css_alter()": "https://www.drupal.org/files/issues/2022-05-02/webform-3278191-3.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
          "bower-asset",
          "npm-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/slick": ["npm-asset/slick-carousel"],
            "web/libraries/ace": ["npm-asset/ace-builds"],
            "web/libraries/{$name}": [
              "type:drupal-library",
              "type:bower-asset",
              "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.htaccess": false
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
                {"name": "dropzone","package": "npm-asset/dropzone"},
                {"name": "blazy", "package": "npm-asset/blazy"},
                {"name": "slick", "package": "npm-asset/slick-carousel"},
                {"name": "ace", "package": "npm-asset/ace-builds"}
            ]
        }
    }
}
